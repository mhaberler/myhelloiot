{
  "name": "myhelloiot",
  "private": true,
  "version": "1.1.7",
  "type": "module",
  "homepage": "https://mhaberler.github.io/myhelloiot/",
  "scripts": {
    "predeploy": "yarn build",
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "littlefs": "tsc && vite build  --base=/apps/myhelloiot/ --emptyOutDir --outDir ../../data/apps/myhelloiot",
    "deploy": "tsc &&  vite --config vite.host.ts build --base=/apps/myhelloiot/&& rsync -avz --delete dist/ mah@mah.priv.at:/var/www/static.mah.priv.at/apps/myhelloiot/",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite --host 0.0.0.0 preview",
    "generate-pwa-assets": "pwa-assets-generator --preset minimal-2023 public/logo.svg"
  },
  "dependencies": {
    "@babel/standalone": "^7.22.20",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@react-three/fiber": "^8.16.8",
    "@reduxjs/toolkit": "^1.9.5",
    "antd": "^5.15.4",
    "buffer": "^6.0.3",
    "gh-pages": "^3.2.0",
    "leaflet": "^1.9.4",
    "mqtt": "^5.2.1",
    "mqtt-match": "^3.0.0",
    "react": "^18.2.0",
    "react-compass-component": "^1.0.11",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-redux": "^8.1.2",
    "react-style-normalizer": "^1.2.8",
    "three": "^0.165.0",
    "uplot": "^1.6.30",
    "uplot-react": "^1.1.5",
    "victory": "^36.6.11",
    "vite-plugin-compression2": "^1.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/babel__standalone": "^7.1.5",
    "@types/leaflet": "^1.9.12",
    "@types/node": "^17.0.45",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "sharp": "^0.33.4",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-pwa": "^0.16.7",
    "vite-plugin-svgr": "^4.1.0"
  }
}
